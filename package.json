{
	"name": "go-snippets",
	"displayName": "go snippets",
	"description": "Golang snippets for vscode",
	"version": "0.0.1",
	"publisher": "qida",
	"icon": "images/golang.png",
	"repository": {
		"url": "https://github.com/qida/vscode-go-snippets"
	},
	"author": {
		"name": "qida",
		"email": "qidasun@gmail.com"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": ["Snippets"],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "go",
				"extensions": [
					".go"
				]
			}
		],
		"snippets": [
			{
				"language": "go",
				"path": "go.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run readme",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js",
		"readme":"node ./src/updateReadme.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"fs": "0.0.1-security",
		"path": "^0.12.7"
	}
}
