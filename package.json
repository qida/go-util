{
  "name": "qida-go-snippets",
  "displayName": "code-snippets",
  "description": "vscode code snippets",
  "version": "0.0.2",
  "publisher": "qida",
  "icon": "images/golang.jpg",
  "repository": {
    "url": "https://github.com/qida/vscode-go-snippets"
  },
  "author": {
    "name": "qida",
    "email": "qidasun@gmail.com"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Snippets"
  ],
  "scripts": {
    "readme": "node ./src/README.js",
    "install": "node ./src/INSTALL.js",
    "build": "vsce package ",
    "all": "npm run readme && npm run build && npm run install"
  },
  "contributes": {
    "snippets": [
      {
        "language": "go",
        "path": "./snippets/beego.code-snippets"
      },
      {
        "path": "./snippets/flutter.code-snippets"
      },
      {
        "path": "./snippets/git.code-snippets"
      },
      {
        "language": "go",
        "path": "./snippets/go.code-snippets"
      },
      {
        "language": "html",
        "path": "./snippets/html.code-snippets"
      },
      {
        "path": "./snippets/linux.code-snippets"
      },
      {
        "path": "./snippets/snippets.code-snippets"
      },
      {
        "language": "go",
        "path": "./snippets/thrift_go.code-snippets"
      }
    ]
  },
  "devDependencies": {
    "node-cmd": "^5.0.0"
  }
}
